    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.41",
    "@types/protractor": "^4.0.0",
    "@types/selenium-webdriver": "^2.53.39",
    "codelyzer": "^2.0.0-beta.4",
    "rollup": "^0.41.6",
    "rxjs": "^5.2.0",
    "tslint": "^4.4.2",
    "typescript": "^2.2.2",
    "uglify-js": "^2.8.21",
    "zone": "^0.3.4",
    "zone.js": "^0.7.6"
  },
  "engines": {
    "node": ">=4.4.1"
  }
}
