    "type": "git",
    "url": "https://github.com/fusioncharts/angular2-fusioncharts"
  },
  "author": {
    "name": "FusionCharts",
    "email": "support@fusioncharts.com"
  },
  "contributors": [
    {
      "name": "Rohit Kumar",
      "email": "rohit.kr001@gmail.com",
      "url": "https://github.com/rohitkr"
    }
  ],
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fusioncharts/angular2-fusioncharts/issues"
  },
  "main": "./dist/index.js",
  "dependencies": {
    "@types/fusioncharts": "^3.11.0",
    "fusioncharts": "^3.11.3-sr.1"
  },
  "devDependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.10",
    "@angular/compiler-cli": "^2.4.10",
    "@angular/core": "^2.4.10",
